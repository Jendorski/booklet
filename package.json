{
    "name": "booklet",
    "version": "1.0.0",
    "description": "Repository that showcases how a shortlet can be developed",
    "main": "index.js",
    "scripts": {
        "build": "rimraf ./build && tsc",
        "test": "mocha",
        "start": "node --trace-warnings build/startup.js",
        "migrate": "pnpm build && pnpm start",
        "swagger": "tsoa spec && tsoa routes",
        "lint": "eslint ./src --no-cache",
        "tsc": "tsc --noEmit",
        "build:test:watch": "pnpm build && pnpm test:watch",
        "test:run": "docker compose -f docker-compose-test.yml up --force-recreate --exit-code-from backend",
        "test:watch": "pnpm test && tsx watch src/startup.ts --clear-screen=false",
        "down": "docker compose down --remove-orphans"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jendorski/booklet.git"
    },
