{
    "name": "booklet",
    "version": "1.0.0",
    "description": "Repository that showcases how a shortlet can be developed",
    "main": "index.js",
    "scripts": {
        "build": "rimraf ./build && tsc",
        "test": "mocha",
        "start": "node --trace-warnings build/startup.js",
        "start:dev": "npm run build && tsx watch src/startup.ts --clear-screen=false",
        "dev": "concurrently \"nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node src/startup.ts'\" \"nodemon -x tsoa spec\"",
        "swagger": "tsoa spec && tsoa routes",
        "sb": "npm run swagger && npm run build",
        "lint": "eslint ./src --no-cache",
        "lint:sb": "npm run lint && npm run sb",
        "tsc": "tsc --noEmit",
        "build:test:watch": "npm run build && npm run test:watch",
        "test:run": "docker compose -f docker-compose-test.yml up --force-recreate --exit-code-from backend",
        "test:watch": "npm run test && tsx watch src/startup.ts --clear-screen=false",
        "test:run:wait": "docker compose -f docker-compose-test.yml -f docker-compose-test-watch.yml up --attach backend --force-recreate",
        "up": "docker compose up --wait",
        "up:dev": "docker compose -f docker-compose.yml -f docker-compose-cache.yml up --attach backend",
        "down": "docker compose down --remove-orphans",
        "knip": "knip"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jendorski/booklet.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Jendorski/booklet/issues"
    },
    "homepage": "https://github.com/Jendorski/booklet#readme",
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
